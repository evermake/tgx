{
  "name": "@telegum/tgx",
  "type": "module",
  "version": "0.3.1",
  "packageManager": "pnpm@9.12.3",
  "description": "JSX runtime that renders Telegram messages content.",
  "author": "Vladislav Deryabkin",
  "license": "MIT",
  "homepage": "https://github.com/telegum/tgx",
  "repository": {
    "type": "git",
    "url": "https://github.com/telegum/tgx"
  },
  "keywords": [
    "telegram",
    "jsx",
    "telegram-bot"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js",
      "require": "./dist/jsx-runtime.cjs"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js",
      "require": "./dist/jsx-runtime.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest --typecheck",
    "test:cov": "vitest --typecheck --coverage",
    "test:ui": "vitest --typecheck --ui",
    "test:ui:cov": "vitest --typecheck --coverage --ui",
    "deps": "taze -wI",
    "release": "bumpp"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@vitest/coverage-v8": "^2.1.4",
    "@vitest/ui": "^2.1.4",
    "bumpp": "^9.8.0",
    "eslint": "^9.14.0",
    "eslint-plugin-format": "^0.1.2",
    "taze": "^0.18.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
